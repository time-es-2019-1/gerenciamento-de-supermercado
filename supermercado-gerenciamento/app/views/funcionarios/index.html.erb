<p id="notice"><%= notice %></p>

<h1>Funcionarios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Datanascimento</th>
      <th>Numtelefone</th>
      <th>Salario</th>
      <th>Cargo</th>
      <th>Dataadimissao</th>
      <th>Datapagamento</th>
      <th>Bairro</th>
      <th>Nomerua</th>
      <th>Nomecidade</th>
      <th>Cep</th>
      <th>Numresidencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funcionarios.each do |funcionario| %>
      <tr>
        <td><%= funcionario.nome %></td>
        <td><%= funcionario.cpf %></td>
        <td><%= funcionario.dataNascimento %></td>
        <td><%= funcionario.numTelefone %></td>
        <td><%= funcionario.salario %></td>
        <td><%= funcionario.cargo %></td>
        <td><%= funcionario.dataAdimissao %></td>
        <td><%= funcionario.dataPagamento %></td>
        <td><%= funcionario.bairro %></td>
        <td><%= funcionario.nomeRua %></td>
        <td><%= funcionario.nomeCidade %></td>
        <td><%= funcionario.cep %></td>
        <td><%= funcionario.numResidencia %></td>
        <td><%= link_to 'Show', funcionario %></td>
        <td><%= link_to 'Edit', edit_funcionario_path(funcionario) %></td>
        <td><%= link_to 'Destroy', funcionario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Funcionario', new_funcionario_path %>
